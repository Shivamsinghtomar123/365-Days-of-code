class Solution {
public:
    bool arrayStringsAreEqual(vector<string>& word1, vector<string>& word2) {
        string str1, str2;

        // Concatenate all strings in word1 into str1
        for (const string& word : word1) {
            str1 += word;
        }

        // Concatenate all strings in word2 into str2
        for (const string& word : word2) {
            str2 += word;
        }

        // Compare the concatenated strings
        return str1 == str2;
    }
};
